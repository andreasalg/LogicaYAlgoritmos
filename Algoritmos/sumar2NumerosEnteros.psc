Proceso sumar2NumerosEnteros
	// declaración de variables 
	// entrada
	Definir num1, num2, suma Como Entero;
	// Proceso 
	Escribir 'Ingrese el primer número:';
	Leer num1;
	Escribir 'Ingrese el segundo número:';
	Leer num2;
	suma <- num1+num2;
	Escribir 'La suma de ', num1, ' y ', num2, ' da: ', suma;
FinProceso
