Proceso restarDosNumerosEnteros
	// declaración de variables 
	// entrada
	Definir num1, num2, resta Como Entero;
	// Proceso 
	Escribir 'Ingrese el primer número:';
	Leer num1;
	Escribir 'Ingrese el segundo número:';
	Leer num2;
	resta <- num1-num2;
	Escribir 'La resta de ', num1, ' y ', num2, ' da: ', resta;
FinProceso
