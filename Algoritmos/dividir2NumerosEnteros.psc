Proceso dividirDosNumerosEnteros
	// declaración de variables 
	// entrada
	Definir num1, num2, division Como Entero;
	// Proceso 
	Escribir 'Ingrese el primer número:';
	Leer num1;
	Escribir 'Ingrese el segundo número:';
	Leer num2;
	division <- num1/num2;
	Escribir 'El resultado de la división ', num1, ' entre ', num2, ' es: ', division;
FinProceso
